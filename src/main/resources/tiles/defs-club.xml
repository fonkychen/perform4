<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
  <definition name="club-template" template="/default/common/topic-layout.jsp">
    <put-attribute name="head" value="/default/club/head.jsp"></put-attribute>
    <put-attribute name="top" value="/default/common/top.jsp"></put-attribute>
    <put-attribute name="breadcrumb" value="/default/common/breadcrumb.jsp"></put-attribute>
    <put-attribute name="side-nav" value="/default/common/side-nav.jsp"></put-attribute>
    <put-attribute name="foot" value="/default/common/foot.jsp"></put-attribute>
  </definition>
  
  <definition name="club/topic" extends="club-template">
    <put-attribute name="title" value="随便说说 —— KPI 365，充满乐趣的互动社区" cascade="true"></put-attribute>
    <put-attribute name="content" value="/default/club/topic.jsp"></put-attribute>
  </definition>
  
  <definition name="club/topic/*" extends="club-template">
    <put-attribute name="title" value="随便说说 —— KPI 365，充满乐趣的互动社区" cascade="true"></put-attribute>
    <put-attribute name="content" value="/default/club/topic.jsp"></put-attribute>
  </definition>
  
  <definition name="club/from" extends="club-template">
    <put-attribute name="title" value="随便说说 —— KPI 365，充满乐趣的互动社区" cascade="true"></put-attribute>
    <put-attribute name="content" value="/default/club/topic.jsp"></put-attribute>
  </definition>
  
  <definition name="club/notice" extends="club-template">
    <put-attribute name="title" value="公告栏 —— KPI 365，充满乐趣的互动社区" cascade="true"></put-attribute>
    <put-attribute name="content" value="/default/club/notice.jsp"></put-attribute>
  </definition>
</tiles-definitions>