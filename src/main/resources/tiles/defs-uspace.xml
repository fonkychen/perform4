<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
  <definition name="uspace-template" template="/default/uspace/space-layout.jsp">
    <put-attribute name="head" value="/default/uspace/head.jsp"></put-attribute>
    <put-attribute name="top" value="/default/common/top.jsp"></put-attribute>
    <put-attribute name="userinfo" value="/default/uspace/userinfo.jsp"></put-attribute>
    <put-attribute name="side-nav" value="/default/common/side-nav.jsp"></put-attribute>
    <put-attribute name="foot" value="/default/common/foot.jsp"></put-attribute>
  </definition>
  
  <definition name="uspace/mallcheck/*" extends="uspace-template">
    <put-attribute name="title" value="消息中心" cascade="true"></put-attribute>
    <put-attribute name="content" value="/default/uspace/mallcheck.jsp"></put-attribute>
  </definition>
</tiles-definitions>